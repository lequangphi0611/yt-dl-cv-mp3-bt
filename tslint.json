{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": true,
        "no-async-promise-executor": true,
        "no-await-in-loop": true,
        "no-duplicate-case": true,
        "no-setter-return": true,
        "dot-location": true,
        "eqeqeq": true,
        "no-else-return": true
    },
    "rulesDirectory": []
}